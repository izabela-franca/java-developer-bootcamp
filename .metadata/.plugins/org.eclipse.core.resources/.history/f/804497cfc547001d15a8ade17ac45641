package logica.variaveis;

public class Main {

	public static void main(String[] args) {
		//Atribuindo variáveis
		int idade;
		idade = 20;
		
		//Outra forma de atribuir variáveis
		int ano = 2022;	//inteiros
		double nota = 9.4;	//decimais
		boolean ativo = false;
		String nome = "Izabela";
		
		System.out.println(nome + " -> " + nota);
		
		//Declarando variáveis de sem determinar seu tipo
		var cor = "azul";
		var numero = 14;

	}

}
